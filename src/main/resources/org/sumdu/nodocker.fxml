<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.sumdu.controllers.NoDockerController">
   <children>
      <Label layoutX="22.0" prefHeight="84.0" prefWidth="383.0" text="Docker is not installed. Go to website:">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="DockerWebsite" editable="false" layoutX="46.0" layoutY="72.0" prefHeight="26.0" prefWidth="313.0" text="https://www.docker.com">
         <font>
            <Font name="System Italic" size="26.0" />
         </font>
      </TextField>
      <Button fx:id="CopyButton" layoutX="106.0" layoutY="145.0" mnemonicParsing="false" onMouseClicked="#onCopyClick" text="Copy to clipboard">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
