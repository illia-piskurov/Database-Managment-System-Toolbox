<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.sumdu.controllers.NewController">
   <children>
      <Button fx:id="CancelButton" layoutX="386.0" layoutY="164.0" mnemonicParsing="false" onMouseClicked="#cancelButtonClicked" prefHeight="26.0" prefWidth="100.0" text="Cancel" />
      <Button fx:id="Create" layoutX="271.0" layoutY="164.0" mnemonicParsing="false" onMouseClicked="#createButtonClicked" prefHeight="26.0" prefWidth="100.0" text="Create" />
      <Label fx:id="NameLabel" layoutX="19.0" layoutY="61.0" text="Name">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="NameText" layoutX="96.0" layoutY="60.0" prefHeight="26.0" prefWidth="178.0" />
      <Label fx:id="TitleLabel" layoutX="134.0" layoutY="14.0" text="Create new database server">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label fx:id="DatabaseLabel" layoutX="290.0" layoutY="60.0" text="DB">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <ComboBox fx:id="DatabaseComboBox" layoutX="328.0" layoutY="61.0" onAction="#onDatabaseComboBoxAction" prefWidth="150.0" />
      <Label fx:id="PasswordLabel" layoutX="19.0" layoutY="100.0" text="Password">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="PasswordText" layoutX="96.0" layoutY="100.0" prefHeight="26.0" prefWidth="178.0" />
      <Label fx:id="PortLabel" layoutX="285.0" layoutY="100.0" text="Port">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="PortText" layoutX="328.0" layoutY="99.0" prefHeight="26.0" prefWidth="150.0" />
   </children>
</AnchorPane>
